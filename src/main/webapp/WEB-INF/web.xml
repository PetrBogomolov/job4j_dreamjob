<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>PostSave</servlet-name>
    <servlet-class>ru.job4j.dreamjob.servlet.PostSave</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CandidateSave</servlet-name>
    <servlet-class>ru.job4j.dreamjob.servlet.CandidateSave</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PostSave</servlet-name>
    <url-pattern>/post/save</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CandidateSave</servlet-name>
    <url-pattern>/candidate/save</url-pattern>
  </servlet-mapping>
</web-app>
